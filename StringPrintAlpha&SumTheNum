//*Print the alpahbets and sum of number*/

import java.util.*;

class Codechef {
    public static void main(String[] args) throws java.lang.Exception {
        String s = "Subha3sh5in7i";
        int numSum = 0;
        StringBuilder alpha = new StringBuilder();
        
        for (char ch : s.toCharArray()) {
            if (Character.isLetter(ch)) {
                alpha.append(ch);
            } else if (Character.isDigit(ch)) {
                numSum += Character.getNumericValue(ch);
            }
        }
        
        System.out.println(alpha.toString() + numSum);
        System.out.println(numSum);
    }
}
// Second easy but hard coded 48 value Ascii value of Character 0 is 48.

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		String s = "Subha3sh5in7i";
		int numSum = 0;
		String alpha = "";
		for(int i =0 ; i<s.length();i++){
		    char ch = s.charAt(i);
    
                    if((ch>='A' && ch<='Z') || (ch>='a'&& ch<='z'))
                    {
                            alpha+=ch; 
                    }
                    
                    else if(ch>='0' && ch<='9'){
                        numSum=numSum+ch-48;
                    }
            }
            
        System.out.println(alpha+numSum);
        
        System.out.println(numSum);
	}
}

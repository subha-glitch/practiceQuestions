import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		int [] arr = {2, 3, 4, 5, 1};
		int k = 7;
		
	    for (int i =0 ; i < arr.length-1 ; i++ ){
	        for(int j =i+1 ; j< arr.length ; j++){
	            if(arr[i]+arr[j]==7)
	            {
	                System.out.printf("%d, %d%n", arr[i], arr[j]);
	            }
	            
	        }
	    } 

	}
}

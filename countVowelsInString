import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		String a = "Subhashini";
		int count =0;
		for(int i =0 ; i<a.length(); i++){
		    if(a.charAt(i)=='a' || a.charAt(i)=='e' || a.charAt(i)=='i' || a.charAt(i)=='o' || a.charAt(i)=='u'){
		        count++;
		    }
		}
		
		System.out.println(count);

	}
}
